{
	"name": "CSCRM",
	"version": "0.1.0",
	"private": true,
	"homepage": "./",
	"main": "public/electron.js",
	"dependencies": {
		"@material-ui/core": "^3.9.2",
		"@material-ui/icons": "^3.0.2",
		"csv-parse": "^4.4.0",
		"csv-parser": "^2.2.0",
		"d3": "^5.9.2",
		"electron-devtools-installer": "^2.2.4",
		"electron-is-dev": "^1.0.1",
		"json-2-csv": "^3.5.0",
		"react": "^16.8.5",
		"react-dev-utils": "^8.0.0",
		"react-dom": "^16.8.5",
		"react-redux": "^7.0.1",
		"redux": "^4.0.1",
		"survey-react": "^1.0.76"
	},
	"scripts": {
		"dev": "yarn react-scripts start",
		"build": "react-scripts build ; cp ./dist-package.json ./build/package.json; cp ./main.js ./build/main.js; cp ./preload.js ./build/preload.js; cp ./state-handler.js ./build/state-handler.js; npm --prefix ./build install ./build",
		"start": "concurrently \"cross-env BROWSER=none yarn react-scripts start\" \"wait-on http://localhost:3000 && electron .\"",
		"pack-macos": "electron-packager ./build CSCRM --out=./dist --platform=darwin --arch=x64 --overwrite --osx-sign.identity='MAYA Design, Inc.'",
		"pack-win": "electron-packager ./build CSCRM --out=./dist --platform=win32 --arch=x64 --overwrite",
		"pack-linux": "electron-packager ./build CSCRM --out=./dist --platform=linux --arch=x64 --overwrite"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"concurrently": "^4.1.0",
		"cross-env": "^5.2.0",
		"electron": "^4.1.4",
		"electron-builder": "^20.39.0",
		"electron-packager": "^13.1.1",
		"react-scripts": "^2.1.8",
		"version-bump-prompt": "^5.0.1"
	}
}
